page{
  width: 100%;
  height: 100%;
  background-color:rgba(245,245,245,0.8)
}
.outer{
  height: 100%;
}
.achievement-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.achievement-item {
  margin: 20rpx;
  padding: 20rpx 20rpx;
  border:1rpx solid rgba(0,0,0,0.5);
  border-radius:10rpx;
  background-color:#ffffff
}
.note-list {
  padding: 40rpx;
}
.note-item{
  float: left;
  margin: 20rpx;
  padding: 20rpx 20rpx;
  border:1rpx solid rgba(0,0,0,0.5);
  border-radius:10rpx;
  background-color:#ffffff;
  width:30%;
}
.left{
  float: left;
  width:33.3333%;
  height: 100%;

}
.right{
  float: right;
  width:66.6666%;
  height: 100%;
  background-color: rgba(220,220,220,0.8)
}